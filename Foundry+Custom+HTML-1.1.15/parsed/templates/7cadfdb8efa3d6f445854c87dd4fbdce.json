{"components/products/product/additional-details":"<div class=\"single-product-meta-wrap\">\n  {{#if product.stock_level}}\n    <div class=\"single-product-meta-item single-product-meta-item-stock\">\n      <span class=\"single-product-meta-item-label\">{{lang 'products.current_stock'}}:</span>\n      <span class=\"single-product-meta-item-value\">{{product.stock_level}}</span>\n    </div>\n  {{/if}}\n  {{#if product.sku}}\n    <div class=\"single-product-meta-item single-product-meta-item-sku\">\n      <span class=\"single-product-meta-item-label\">{{lang 'product.sku'}}:</span>\n      <span class=\"single-product-meta-item-value\" data-product-sku>{{product.sku}}</span>\n    </div>\n  {{/if}}\n  {{#if product.upc}}\n    <div class=\"single-product-meta-item single-product-meta-item-upc\">\n      <span class=\"single-product-meta-item-label\">{{lang 'product.upc'}}:</span>\n      <span class=\"single-product-meta-item-value\" data-product-upc>{{product.upc}}</span>\n    </div>\n  {{/if}}\n  {{#if product.release_date}}\n    <div class=\"single-product-meta-item single-product-meta-item-release-date\">\n      <span class=\"single-product-meta-item-label\">{{lang 'product.release_date'}}:</span>\n      <span class=\"single-product-meta-item-value\">{{product.release_date}}</span>\n    </div>\n  {{/if}}\n\n  {{#if product.width}}\n    <div class=\"single-product-meta-item single-product-meta-item-size\">\n      <span class=\"single-product-meta-item-label\">{{lang 'product.size.label'}}:</span>\n      <span class=\"single-product-meta-item-value\">\n        {{#if product.width}}\n          {{product.width}}\n          {{lang 'product.size.width'}}\n        {{/if}}\n        &times;\n        {{#if product.height}}\n          {{product.height}}\n          {{lang 'product.size.height'}}\n        {{/if}}\n        &times;\n        {{#if product.depth}}\n          {{product.depth}}\n          {{lang 'product.size.length'}}\n        {{/if}}\n      </span>\n    </div>\n  {{/if}}\n\n  {{#if product.condition}}\n    <div class=\"single-product-meta-item single-product-meta-item-condition\">\n      <span class=\"single-product-meta-item-label\">{{lang 'product.condition'}}:</span>\n      <span class=\"single-product-meta-item-value\">{{product.condition}}</span>\n    </div>\n  {{/if}}\n  {{#if product.availability}}\n    <div class=\"single-product-meta-item single-product-meta-item-availability\">\n      <span class=\"single-product-meta-item-label\">{{lang 'product.availability'}}:</span>\n      <span class=\"single-product-meta-item-value\">{{product.availability}}</span>\n    </div>\n  {{/if}}\n  {{#if product.weight}}\n    <div class=\"single-product-meta-item single-product-meta-item-weight\">\n      <span class=\"single-product-meta-item-label\">{{lang 'product.weight'}}:</span>\n      <span class=\"single-product-meta-item-value\" data-product-weight>{{product.weight}}</span>\n    </div>\n  {{/if}}\n  {{#if product.gift_wrapping_available}}\n    <div class=\"single-product-meta-item single-product-meta-item-gift-wrapping\">\n      <span class=\"single-product-meta-item-label\">{{lang 'product.gift_wrapping'}}:</span>\n      <span class=\"single-product-meta-item-value\">{{lang 'product.gift_wrapping_available'}}</span>\n    </div>\n  {{/if}}\n  <!--{{#if product.warranty}}-->\n  <!--  <div class=\"single-product-meta-item single-product-meta-item-warranty\">-->\n  <!--    <span class=\"single-product-meta-item-label\">{{lang 'DETAILS'}}:</span>-->\n  <!--    {{{product.description}}}-->\n\n  <!--            {{{snippet 'product_description'}}}-->\n  <!--  </div>-->\n  <!--{{/if}}-->\n  {{#if product.description}}\n          <div class=\"tabs-content-panel\" id=\"product-details\">\n            <div class=\"product-description rte\">\n                <span class=\"single-product-meta-item-value\">{{nl2br product.tabs.warranty}}</span>\n              <!--{{{product.description}}}-->\n\n              <!--{{{snippet 'product_description'}}}-->\n            </div>\n          </div>\n        {{/if}}\n  {{#if product.shipping}}\n    {{#if product.shipping.calculated}}\n      <div class=\"single-product-meta-item single-product-meta-item-shipping\">\n        <span class=\"single-product-meta-item-label\">{{lang 'product.shipping'}}:</span>\n        <span class=\"single-product-meta-item-value\">{{lang 'product.shipping_calculated'}}</span>\n      </div>\n    {{else}}\n      {{#compare product.shipping.price 0 operator='==='}}\n        <div class=\"single-product-meta-item single-product-meta-item-shipping\">\n          <span class=\"single-product-meta-item-label\">{{lang 'product.shipping'}}:</span>\n          <span class=\"single-product-meta-item-value\">{{lang 'product.shipping_free'}}</span>\n        </div>\n      {{else}}\n        <div class=\"single-product-meta-item single-product-meta-item-shipping\">\n          <span class=\"single-product-meta-item-label\">{{lang 'product.shipping'}}:</span>\n          <span class=\"single-product-meta-item-value\">{{lang 'product.shipping_fixed' amount=product.shipping.price.formatted}}</span>\n        </div>\n      {{/compare}}\n    {{/if}}\n  {{/if}}\n  {{#each product.custom_fields}}\n    <div class=\"single-product-meta-item single-product-meta-item-custom-fields\">\n      <span class=\"single-product-meta-item-label\">{{name}}:</span>\n      <span class=\"single-product-meta-item-value\">{{value}}</span>\n    </div>\n  {{/each}}\n  {{#if product.bulk_discount_rates.length}}\n    <div class=\"single-product-meta-item single-product-meta-item-bulk-discounts single-product-bulk-discounts\">\n      <span class=\"single-product-meta-item-label\">{{lang 'product.bulk_pricing.title'}}</span>\n      <span class=\"single-product-meta-item-value\">\n        <a href=\"#\" class=\"product-bulk-pricing-toggle\">{{lang 'product.bulk_pricing.see_pricing'}}</a>\n      </span>\n    </div>\n  {{/if}}\n\n  {{#if product.detail_messages}}\n    <div class=\"single-product-meta-item single-product-meta-item-detail-messages\">\n      <span class=\"single-product-meta-item-label\">{{lang 'product.message'}}</span>\n      <span class=\"single-product-meta-item-value\">\n        {{product.detail_messages}}\n      </span>\n    </div>\n  {{/if}}\n</div>\n"}
